{
    "name": "cardpay/php-sdk-v3",
    "version": "3.11.3",
    "description": "PHP SDK for Unlimint API v3",
    "keywords": [
        "unlimint",
        "api",
        "php",
        "sdk"
    ],
    "homepage": "https://integration.unlimint.com",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "Cardpay\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Cardpay\\test\\" : "test/" }
    }
}